{
   "manifest_version": 2,
   "name": "Jeep Weather Forecast",
   "version": "0.0.0.1",

   "description": "This extension shows the if there is dry weather ahead to Jeeping with the top down.",

   "browser_action": 
   { 
      "default_icon": "closed.png", 
      "default_popup": "popup.html" 
   }, 

   "permissions": 
   [ 
      "geolocation", 
      "storage", 
      "https://jeepweather.bump.us/" 
   ] 
}

